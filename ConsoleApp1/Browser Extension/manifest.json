{
  "manifest_version": 3,
  "version": "1.0",
  "name": "Character Ai Dialog Export",
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "downloads",
    "scripting"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [ "<all_urls>" ],
      "js": [ "content.js" ]
    }
  ],
  "host_permissions": [ "<all_urls>" ]
}